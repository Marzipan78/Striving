{
  "manifest_version": 2,
  "name": "Stampify",
  "version": "0.2",
  "background": {
    "scripts": [ "background.js" ]
,
    "persistent": false
  },
  
  

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Stampify",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "http://127.0.0.1/",
    "https://www.google.com/*"
  ],
  "icons": {
    "16": "icon.png"
  }
}


